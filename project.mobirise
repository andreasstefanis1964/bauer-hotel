ion=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"bg.type!='color' && overlay\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"content-wrap col-12\">\n                <h1 class=\"mbr-section-title mbr-fonts-style mbr-white\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">Entschleunigen Sie sich in ruhiger Lage!</h1>\n                <div class=\"text-wrap\" mbr-if=\"showText || showText\">\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style mbr-white mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-subtitle\"></h5>\n                    <p class=\"mbr-fonts-style mbr-text mbr-white mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><br>\n                        hellolodgem5@gmail.com\n                    </p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
